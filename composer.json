{
    "name": "phpmob/api-bundle",
    "type": "symfony-bundle",
    "description": "APIs management for Symfony projects.",
    "keywords": ["api", "rest"],
    "homepage": "https://github.com/phpmob/api-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "homepage": "https://github.com/liverbool"
        }
    ],
    "require": {
        "php": "^7.1",

        "friendsofsymfony/oauth-server-bundle": "^1.5",
        "sylius/resource-bundle": "^1.0",
        "symfony/form": "^3.2",
        "symfony/framework-bundle": "^3.2",
        "symfony/validator": "^3.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "phpspec/phpspec": "^3.1",
        "phpunit/phpunit": "^5.6",
        "twig/twig": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "PhpMob\\ApiBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "PhpMob\\ApiBundle\\spec\\": "spec/" }
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
